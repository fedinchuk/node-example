{
  "name": "example",
  "type": "module",
  "version": "1.0.0",
  "description": "learn node",
  "main": "dist/bundle.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "node dist/bundle.js",
    "dev": "rollup -c -w",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "tw": "npx tailwindcss -i ./src/index.css -o ./dist/bundle.css --watch",
    "bundle": "rollup -c -w && npx tailwindcss -i ./src/index.css -o ./dist/bundle.css --watch",
    "test": "jest"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ],
    "moduleNameMapper": {
      "\\.module\\.css$": "<rootDir>/__mocks__/cssModuleMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/test/",
      "<rootDir>/e2e/",
      "<rootDir>/tests-examples"
    ],
    "testEnvironment": "jsdom"
  },
  "author": "Vasyl Fedinchuk",
  "license": "ISC",
  "homepage": "https://fedinchuk.github.io/node-example",
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.1",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-spinners": "^0.13.8",
    "react-switch": "^7.0.0",
    "redux": "^5.0.1",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/plugin-transform-arrow-functions": "^7.23.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.23.3",
    "@chromatic-com/storybook": "^1.2.25",
    "@playwright/test": "^1.42.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/addon-onboarding": "^8.0.4",
    "@storybook/blocks": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@storybook/react-vite": "^8.0.4",
    "@storybook/test": "^8.0.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/node": "^20.12.3",
    "babel-jest": "^29.7.0",
    "cypress": "^13.7.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "gh-pages": "^6.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "prop-types": "^15.8.1",
    "rollup": "^4.12.0",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^8.0.4",
    "tailwindcss": "^3.4.3"
  },
  "keywords": []
}
